<svg width="{{width}}" 
     height="{{height}}" 
     viewBox="{{viewBox}}" 
     fill="none" 
     xmlns="http://www.w3.org/2000/svg" 
     class="{{className}}">
    <path d="{{dPath}}" 
          stroke="{{stroke}}" 
          stroke-linejoin="{{strokeLinejoin}}" 
          fill="{{fill}}"/>
    <circle cx="{{cx}}" 
            cy="{{cy}}" 
            r="{{r}}" 
            stroke="{{strokeCircle}}"/>
</svg>